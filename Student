public static class Student {
        private int grade = 5;
        private String name = "Defoult";

        public Student(String name, Integer grade) {
            this.name = name;
            this.grade = grade;
        }

        public String getName() {
            return name;
        }

        public int getGrade() {
            return grade;
        }

        public String announce() {
            return (name + "учится в" + grade + "классе!");
        }
    }
